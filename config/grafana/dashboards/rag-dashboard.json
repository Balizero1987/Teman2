{
  "dashboard": {
    "title": "Nuzantara RAG Performance Dashboard",
    "tags": ["rag", "performance", "slo"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 2,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "RAG Pipeline Latency (P50, P95, P99)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(zantara_rag_pipeline_duration_seconds_bucket[5m]))",
            "legendFormat": "P50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(zantara_rag_pipeline_duration_seconds_bucket[5m]))",
            "legendFormat": "P95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(zantara_rag_pipeline_duration_seconds_bucket[5m]))",
            "legendFormat": "P99"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Latency"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "RAG Query Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(zantara_rag_queries_total[5m])",
            "legendFormat": "{{collection}} - {{route_used}} - {{status}}"
          }
        ],
        "yaxes": [
          {
            "format": "reqps",
            "label": "Queries/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Cache Hit Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(zantara_cache_hits_total[5m]) / (rate(zantara_cache_hits_total[5m]) + rate(zantara_cache_misses_total[5m]))",
            "legendFormat": "Global Cache"
          },
          {
            "expr": "zantara_rag_cache_hit_rate",
            "legendFormat": "RAG Cache"
          }
        ],
        "yaxes": [
          {
            "format": "percentunit",
            "label": "Hit Rate"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Tool Call Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(zantara_rag_tool_calls_total[5m])",
            "legendFormat": "{{tool_name}} - {{status}}"
          }
        ],
        "yaxes": [
          {
            "format": "reqps",
            "label": "Calls/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 5,
        "title": "LLM Fallback Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(zantara_rag_fallback_count_total[5m])",
            "legendFormat": "{{from_model}} â†’ {{to_model}}"
          }
        ],
        "yaxes": [
          {
            "format": "reqps",
            "label": "Fallbacks/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 6,
        "title": "Context Length Distribution",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(zantara_rag_context_length_tokens_bucket[5m]))",
            "legendFormat": "P95 - {{collection}}"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Tokens"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 7,
        "title": "Vector Search Latency",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(zantara_rag_vector_search_duration_seconds_bucket[5m]))",
            "legendFormat": "P95"
          },
          {
            "expr": "histogram_quantile(0.50, rate(zantara_rag_vector_search_duration_seconds_bucket[5m]))",
            "legendFormat": "P50"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Latency"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 8,
        "title": "Embedding Generation Latency",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(zantara_rag_embedding_duration_seconds_bucket[5m]))",
            "legendFormat": "P95"
          },
          {
            "expr": "histogram_quantile(0.50, rate(zantara_rag_embedding_duration_seconds_bucket[5m]))",
            "legendFormat": "P50"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Latency"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      }
    ]
  }
}
